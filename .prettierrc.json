{
  "$schema": "https://json.schemastore.org/prettierrc",
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 160,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketLine": false,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "singleAttributePerLine": false
}
